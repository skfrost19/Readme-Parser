{
  "name": "readme-parser",
  "displayName": "Readme Parser",
  "publisher": "ShahilKumar",
  "description": "A VS Code extension to parse ReadMe files.",
  "icon": "assets/icons/parsing.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/skfrost19/Readme-Parser"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "readme-parser.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "readme-parser.parse",
        "title": "Parse"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "readme-parser.parse",
          "when": "resourceFilename =~ /readme\\.md/i"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "markdown-it": "^13.0.1"
  }
}
